  },
  "commands": [
    {
      "name": "prologue",
      "description": "Show Prologue banner and ecosystem overview",
      "type": "info",
      "aliases": ["p"]
    },
    {
      "name": "prologue-interactive",
      "description": "Launch interactive menu with arrow key navigation",
      "type": "interactive",
      "aliases": ["prologue-i", "pi"]
    },
    {
      "name": "prologue-list",
      "description": "List all available commands and categories",
      "type": "list",
      "aliases": ["prologue-ls"]
    },
    {
      "name": "prologue-install",
      "description": "Install Prologue for specific platform",
      "type": "setup",
      "arguments": ["platform"]
    }
  ],
