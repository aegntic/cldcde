{
  "name": "@aegntic/prologue-plugin",
  "version": "1.0.0",
  "description": "Cross-platform interactive menu system for the @aegntic ecosystem",
  "type": "plugin",
  "platforms": ["claude-code", "factory-droid", "antigravity", "gemini", "opencode", "codex", "kilo", "goose", "cursor", "cli"],
  "entryPoints": {
    "cli": "cli.js",
    "claude-code": "commands/prologue.md",
