{
  "name": "@prologue/cli",
  "version": "1.0.0",
  "description": "Prologue - The AI-Powered Creative Framework for Building Tomorrow's Digital Experiences",
  "main": "dist/index.js",
  "bin": {
    "prologue": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "server": "ts-node src/server.ts",
    "server:dev": "nodemon --exec ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    "prologue",
    "ai-powered",
    "creative-framework",
    "web-development",
    "game-development",
    "interactive-art",
    "ai-assistant",
    "rapid-prototyping",
    "cross-platform",
    "creative-coding",
    "ai-agents",
    "visual-development"
  ],
  "author": "AEGNTIC.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aegntic/prologue.git"
  },
  "bugs": {
    "url": "https://github.com/aegntic/prologue/issues"
  },
  "homepage": "https://logue.pro",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "templates"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "inquirer": "^9.2.0",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "axios": "^1.5.0",
    "node-cron": "^3.0.2",
    "semver": "^7.5.0",
    "tar": "^6.1.0",
    "crypto-js": "^4.1.0",
    "yaml": "^2.3.0",
    "node-fetch": "^3.3.0",
    "dotenv": "^16.3.0",
    "fs-extra": "^11.1.0",
    "which": "^4.0.0",
    "cross-spawn": "^7.0.3",
    "execa": "^8.0.0",
    "uuid": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "tailwindcss": "^3.3.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
