    "nodemon": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/semver": "^7.5.0",
    "@types/tar": "^6.1.0",
    "@types/crypto-js": "^4.1.0",
    "@types/fs-extra": "^11.0.0",
    "@types/which": "^3.0.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/uuid": "^9.0.0",
