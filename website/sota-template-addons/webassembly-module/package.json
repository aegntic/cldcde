{
  "name": "@sota/webassembly-module",
  "version": "1.0.0",
  "description": "High-performance WebAssembly integration module for image processing, video encoding, and complex computations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && webpack --mode=production",
