{
  "name": "@sota/template-sdk",
  "version": "1.0.0",
  "description": "Professional Development Kit for State-of-the-Art Template System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
    "dev": "bun --watch src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "bun run build && bun run test"
  },
  "keywords": [
    "templates",
    "performance",
    "animations",
    "web-assembly",
    "ai",
    "typescript",
    "solid-design"
  ],
  "author": {
    "name": "AEGNTIC AI Ecosystems",
    "email": "research@aegntic.ai",
    "url": "https://aegntic.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aegntic/sota-template-sdk.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/index.js",
      "types": "./dist/vue/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "dependencies": {
    "web-vitals": "^3.5.0",
    "gsap": "^3.12.2",
    "intersection-observer": "^0.12.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "typedoc": "^0.25.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}